<div class="row">
	<div class="col-xs-offset-1 col-xs-10 verticalCenter center links">
		<% if user_signed_in? %>
			<%= link_to current_user do%>
				<h1><%= current_user.username%></h1>
			<%end%>
			<ul>
				<% @all_users.each do |user| %>
					<li><%= user.username%></li>
				<%end%>
			</ul>
		<%else%>
			<div class="twitterSignIn verticalCenter">
				<%= link_to user_omniauth_authorize_path(:twitter), :class=>"btn btn-default btn-xlg" do%>
					<i class="fa fa-twitter"></i> Sign In
				<%end%>
			</div>
		<%end%>
	</div>
</div>